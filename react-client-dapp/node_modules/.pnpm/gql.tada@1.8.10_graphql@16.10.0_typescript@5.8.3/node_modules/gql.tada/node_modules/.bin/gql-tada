#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/gql.tada@1.8.10_graphql@16.10.0_typescript@5.8.3/node_modules/gql.tada/bin/node_modules:/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/gql.tada@1.8.10_graphql@16.10.0_typescript@5.8.3/node_modules/gql.tada/node_modules:/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/gql.tada@1.8.10_graphql@16.10.0_typescript@5.8.3/node_modules:/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/gql.tada@1.8.10_graphql@16.10.0_typescript@5.8.3/node_modules/gql.tada/bin/node_modules:/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/gql.tada@1.8.10_graphql@16.10.0_typescript@5.8.3/node_modules/gql.tada/node_modules:/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/gql.tada@1.8.10_graphql@16.10.0_typescript@5.8.3/node_modules:/Users/ryowakabayashi/work/sui/20250405_othello/othello/react-client-dapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
